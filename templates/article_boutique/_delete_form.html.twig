<form method="post" action="{{ path('app_article_boutique_delete', {'id': articleBoutique.id}) }}" 
      onsubmit="return confirm('Are you sure you want to delete this article?');">

    
    <div>
        <button class="btn btn-danger" type="submit">Delete</button>
    </div>

    <!-- CSRF token for security -->
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ articleBoutique.id) }}">
</form>
